}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-content: center;
  justify-content: space-between;
  box-sizing: border-box;
}

@font-face {
  font-family: 'sleeve';  /* project id 1910001 */
  src: url('//at.alicdn.com/t/font_1910001_ceclb3t2hg8.eot');
  src: url('//at.alicdn.com/t/font_1910001_ceclb3t2hg8.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1910001_ceclb3t2hg8.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1910001_ceclb3t2hg8.woff') format('woff'),
  url('//at.alicdn.com/t/font_1910001_ceclb3t2hg8.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1910001_ceclb3t2hg8.svg#sleeve') format('svg');
}

.sleeve{
  font-family: 'sleeve' !important;
  color: #157658 !important;
}

